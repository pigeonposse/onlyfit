{
	"name": "@onlyfit/web",
	"version": "0.0.2",
	"private": true,
	"description": "Compress your files without losing quality",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"perf": "pnpx unlighthouse --site http:/localhpost:1312",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview --port 1312"
	},
	"dependencies": {
		"browser-image-compression": "^2.0.2",
		"fflate": "^0.8.2",
		"jszip": "^3.10.1",
		"pdf-lib": "^1.17.1",
		"svgo": "^3.3.2"
	},
	"devDependencies": {
		"@svaio/media": "0.0.6",
		"@svaio/meta": "0.0.6",
		"@svaio/pwa": "0.0.6",
		"@svaio/sitemap": "0.0.6",
		"@svaio/unocss": "0.0.6",
		"@sveltejs/adapter-static": "3.0.8",
		"@sveltejs/kit": "2.21.2",
		"svelte": "5.33.17",
		"svelte-check": "4.2.1"
	}
}
