{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "onlyfit",
	"version": "0.1.0",
	"identifier": "com.onlyfit.app",
	"build": {
		"frontendDist": "../../web/build"
	},
	"app": {
		"withGlobalTauri": false,
		"macOSPrivateApi": true,
		"windows": [
			{
				"height": 800,
				"width": 600,
				"minHeight": 600,
				"minWidth": 600,
				"fullscreen": false,
				"resizable": true,
				"hiddenTitle": false,
				"dragDropEnabled": true,
				"transparent": true,
				"title": "Onlyfit"
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
